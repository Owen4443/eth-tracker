{
  "manifest_version": 2,
  "name": "Ethereum Wallet Tracker",
  "version": "1.0",
  "description": "Track Ethereum wallet tokens using Alchemy API.",
  "permissions": [
    "http://localhost:5000/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon.png"
    }
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}